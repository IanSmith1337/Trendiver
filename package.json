{
  "name": "trendiver",
  "version": "0.2.0",
  "description": "A trend-visualization resource.",
  "main": ".webpack/main",
  "scripts": {
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron-forge start",
    "start": "react-scripts start",
    "package": "react-scripts build && electron-forge package",
    "make": "react-scripts build && electron-forge make",
    "publish": "electron-forge publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IanSmith1337/Trendiver.git"
  },
  "keywords": [
    "Trends",
    "Trend",
    "Twitter",
    "Social Media",
    "Hashtags",
    "Data Visualization"
  ],
  "author": "Ian Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IanSmith1337/Trendiver/issues"
  },
  "homepage": "https://github.com/IanSmith1337/Trendiver#readme",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@electron-forge/cli": "^6.0.3",
    "@electron-forge/maker-deb": "^6.0.3",
    "@electron-forge/maker-dmg": "^6.0.3",
    "@electron-forge/maker-rpm": "^6.0.3",
    "@electron-forge/maker-squirrel": "^6.0.3",
    "@electron-forge/maker-zip": "^6.0.3",
    "@electron-forge/plugin-webpack": "^6.0.3",
    "@electron-forge/publisher-github": "^6.0.3",
    "@electron/get": "^2.0.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.5.0",
    "css-loader": "^6.7.2",
    "electron": "^21.2.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.1",
    "wait-on": "^6.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "firebase": "^9.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
